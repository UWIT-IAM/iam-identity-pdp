# global settings for Dajngo demo app


WSGIDaemonProcess pdp_group group=iam-dev python-path={{ app_base }}/pdp:{{ app_base }}/pdp/lib/python2.7/site-packages
WSGIScriptAlias /pdp {{ app_base }}/pdp/pdp/wsgi.py
WSGIProcessGroup pdp_group

<Directory {{ app_base }}/pdp/pdp>
<Files wsgi.py>
        Order deny,allow
        Allow from all
</Files>
</Directory>


